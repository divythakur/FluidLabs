const handlebars = require('handlebars');

// Define your email template
const emailTemplate = `
  <html>
    <body>
      <h1>Hello, {{name}}!</h1>
      <p>This is a test email sent using Handlebars and Nodemailer.</p>
    </body>
  </html>
`;

// Compile the template
const compiledTemplate = handlebars.compile(emailTemplate);

module.export= compiledTemplate;